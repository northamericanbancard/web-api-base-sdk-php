{
  "name": "nab/web-api-base-sdk-php",
  "type": "library",
  "description": "North American Bancard Generic Base-SDK PHP library.",
  "license": "MIT",
  "authors": [
    {
      "name": "James Hollenbeck",
      "email": "jhollenbeck@nabancard.com"
    }
  ],
  "support": {
    "email": "jhollenbeck@nabancard.com"
  },
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "*",
    "aws/aws-sdk-php": "^3.36"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "squizlabs/php_codesniffer": "^3"
  },
  "config": {
    "platform": {
      "php": "5.5.9"
    }
  },
  "autoload": {
    "psr-4": {
      "NAB\\WebApiBaseSdk\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NAB\\Tests\\": "tests/"
    }
  }
}
